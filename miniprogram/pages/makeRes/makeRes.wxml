<!-- miniprogram/pages/makeRes/makeRes.wxml -->
<view class="weui-cells__title">工作经验</view>
<view class="weui-cells weui-cells_after-title">
  <checkbox-group bindchange="selectWork">
    <label class="weui-cell weui-check__label" wx:for="{{workExp}}" wx:key="value">
      <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
      <view class="weui-cell__hd weui-check__hd_in-checkbox">
        <icon class="weui-icon-checkbox_circle" type="circle" size="23" wx:if="{{!item.checked}}"></icon>
        <icon class="weui-icon-checkbox_success" type="success" size="23" wx:if="{{item.checked}}"></icon>
      </view>
      <view class="weui-cell__bd">{{item.name}}</view>
    </label>
  </checkbox-group>
</view>
<view class="weui-cells__title">项目成果</view>
<view class="weui-cells weui-cells_after-title">
  <checkbox-group bindchange="selectItem">
    <label class="weui-cell weui-check__label" wx:for="{{itemDesc}}" wx:key="value">
      <checkbox class="weui-check" value="{{item.value}}" checked="{{item.checked}}" />
      <view class="weui-cell__hd weui-check__hd_in-checkbox">
        <icon class="weui-icon-checkbox_circle" type="circle" size="23" wx:if="{{!item.checked}}"></icon>
        <icon class="weui-icon-checkbox_success" type="success" size="23" wx:if="{{item.checked}}"></icon>
      </view>
      <view class="weui-cell__bd">{{item.name}}</view>
    </label>
  </checkbox-group>
</view>